@*@model BDCO.Domain.QueryResult.AdminDashboardQueryResult*@
@{
    ViewBag.Title = "Home";
}
<link href="~/Content/dashboardmain.min.css" rel="stylesheet" />
<link href="~/Content/uikit.almost-flat.min.css" rel="stylesheet" />
<style>
    body {
        background-color: #ebebeb !important;
    }

    #randerDiv {
        border: none !important;
        box-shadow: none !important;
    }

    .md-card {
        background: #fff;
        position: relative;
        -webkit-box-shadow: 0 1px 3px rgba(0,0,0,.12), 0 1px 2px rgba(0,0,0,.24);
        box-shadow: 0 1px 3px rgba(0,0,0,.12), 0 1px 2px rgba(0,0,0,.24);
        border: none;
    }

    .row {
        margin-top: 25px;
    }

    .k-chart, .k-stockchart {
        height: 250px;
    }

    #example10 {
        height: 142px;
    }

    .k-radio:checked + .k-radio-label:after {
        content: "";
        width: 10px;
        height: 10px;
        position: absolute;
        top: 2px;
        left: 2px;
    }

    .k-radio-label {
        position: relative;
        padding-left: 1.5em;
        vertical-align: middle;
        line-height: 1.4em;
        cursor: pointer;
    }
</style>

<div>

    <hr />
    <form id="block">
        <input type="hidden" name="RecordId" id="RecordId" value="" />
        <div class="row">
            <h2 style="padding-left: 0px;padding-right: 30px;color: #d50808; padding-top:5px;text-align:center;">
                Teacher Professional Development (TPD)
            </h2>
            <div class="col-md-12">
                <h4 style="text-align:center;">“Improving quality of teaching through professional development initiatives across the agencies to support teachers and education stakeholders in Cox’s Bazar Rohingya Response”</h4>
            </div>
            <div class="col-md-12">
                <img src="~/Images/home.png" style="width:100%" />
            </div>
        </div>
        <div class="row" style="padding-top:25px;;">
            <div class="col-md-2">
                <img src="~/Images/unicef.png" />
            </div>
            <div class="col-md-2 col-md-offset-8" style="text-align:right; right:0;">
                <img src="~/Images/unhcr.png" />
            </div>
        </div>
    </form>
</div>