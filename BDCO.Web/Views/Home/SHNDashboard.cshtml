                document.getElementById("HandWashObserved").innerHTML = data.HandWashObserved;
                document.getElementById("HandWashNoCoverage").innerHTML = data.HandWashNoCoverage;


                document.getElementById("DrinkingWaterAchieved").innerHTML = data.DrinkingWaterAchieved + '%';
