            if (result) {
                var data = data.Data;
                GraphManager.createChartServiceTrend("ServiceTrend", "Observed Trend- (Overall)", data.ServiceTrend);


                document.getElementById("TotalBen").innerHTML = data.TotalBen;
                document.getElementById("TotalWomen").innerHTML = data.TotalWomen;
                document.getElementById("TotalMan").innerHTML = data.TotalMan;
                document.getElementById("TotalBoy").innerHTML = data.TotalBoy;
                document.getElementById("TotalGirl").innerHTML = data.TotalGirl;
