                document.getElementById("DrinkingWaterNoCoverage").innerHTML = data.DrinkingWaterNoCoverage;

                document.getElementById("DrinkingWaterChlorineTablet").innerHTML = data.DrinkingWaterChlorineTablet;
                document.getElementById("DrinkingWaterFitkari").innerHTML = data.DrinkingWaterFitkari;
                document.getElementById("DrinkingWaterFilter").innerHTML = data.DrinkingWaterFilter;
                document.getElementById("DrinkingWaterMinaral").innerHTML = data.DrinkingWaterMinaral;
                document.getElementById("DrinkingWaterTubeWell").innerHTML = data.DrinkingWaterTubeWell;

