    // All Graph function start

    function fnLoadDashboard() {
        var urlpath = "";
        urlpath = '@Url.Action("GetSHNDashboardData", "Home")';
        
        var jsonData = {
            'UserId': 0,
            'Year': $year,
            'FromDate': $("#FromDate").val(),
            'ToDate': $("#ToDate").val(),
            'ServicePointId': $("#ServicePointId").val(),
            'DistrictCode': $("#DistrictCode").val(),
            'UpazilaCode': $("#UpazilaCode").val(),

        };
