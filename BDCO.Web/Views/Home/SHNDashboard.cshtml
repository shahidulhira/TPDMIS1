                document.getElementById("ToiletObserved").innerHTML = data.ToiletObserved;
                document.getElementById("ToiletNoCoverage").innerHTML = data.ToiletNoCoverage;
                document.getElementById("Toilet4CriteriaMeet").innerHTML = data.Toilet4CriteriaMeet;
                // int, int,  int,
                document.getElementById("ToiletMale").innerHTML = data.ToiletMale;
                document.getElementById("ToiletFemale").innerHTML = data.ToiletFemale;
                document.getElementById("ToiletBoth").innerHTML = data.ToiletBoth;


                document.getElementById("WasteMangementAchieved").innerHTML = data.WasteMangementAchieved + '%';
                //document.getElementById("WasteMangementAttained").innerHTML = data.WasteMangementAttained;
                document.getElementById("WasteMangementObserved").innerHTML = data.WasteMangementObserved;
                document.getElementById("WasteMangementNoCoverage").innerHTML = data.WasteMangementNoCoverage;
                document.getElementById("WasteMangement3CriteriaMeet").innerHTML = data.WasteMangement3CriteriaMeet;

                document.getElementById("DewormingAchieved").innerHTML = data.DewormingAchieved + '%';
                document.getElementById("DewormingObserved").innerHTML = data.DewormingObserved;
                document.getElementById("TotalChild").innerHTML = data.TotalChild;
                document.getElementById("ServicePointGoingTotal").innerHTML = data.ServicePointGoingTotal+ " ";
                document.getElementById("ServicePointGoingBoy").innerHTML = data.ServicePointGoingBoy;
                document.getElementById("ServicePointGoingGirl").innerHTML = data.ServicePointGoingGirl;
                document.getElementById("OutOfServicePointTotal").innerHTML = data.OutOfServicePointTotal;
                document.getElementById("OutOfServicePointBoy").innerHTML = data.OutOfServicePointBoy;
                document.getElementById("OutOfServicePointGirl").innerHTML = data.OutOfServicePointGirl;
                document.getElementById("SponsoredTotal").innerHTML = data.SponsoredTotal;
                document.getElementById("SponsoredBoy").innerHTML = data.SponsoredBoy;
                document.getElementById("SponsoredGirl").innerHTML = data.SponsoredGirl;
                document.getElementById("SpecialNeedTotal").innerHTML = data.SpecialNeedTotal;
                document.getElementById("SpecialNeedBoy").innerHTML = data.SpecialNeedBoy;
                document.getElementById("SpecialNeedGirl").innerHTML = data.SpecialNeedGirl;
                document.getElementById("SessionTotal").innerHTML = data.SessionTotal;
